{
  "name": "bitqast-api-service",
  "version": "3.2.1",
  "description": "be proxy gateway for bik project",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:debug": "nodemon src/index.js",
    "test": "jest --coverage",
    "dev": "nodemon src/index.js",
    "prod": "node src/index.js",
    "format": "prettier --write .",
    "scancode": "npx sonarqube-scanner",
    "migrate:create": "migrate create --migrations-dir=\"./migrations/files\" --compiler='js:babel-register'",
    "migrate:up": "migrate --migrations-dir=\"./migrations/files\" up --compiler='js:babel-register' --store=\"./migrations/migration-store.js\"",
    "migrate:down": "migrate --migrations-dir=\"./migrations/files\" down --compiler='js:babel-register' --store=\"./migrations/migration-store.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/FutureSpace/bik-project/backend-modules/proxy-gateway-bik-api-service.git"
  },
  "author": "thapakorn613@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/FutureSpace/bik-project/backend-modules/proxy-gateway-bik-api-service/issues"
  },
  "homepage": "https://gitlab.com/FutureSpace/bik-project/backend-modules/proxy-gateway-bik-api-service#readme",
  "dependencies": {
    "@node-saml/passport-saml": "^4.0.2",
    "ajv": "^8.10.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "app-root-path": "^3.0.0",
    "aws-sdk": "^2.1303.0",
    "axios": "^0.25.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csrf-csrf": "^2.2.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "mongoose": "^6.2.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.0",
    "node-yaml-config": "^1.0.0",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.15",
    "passport": "^0.6.0",
    "path": "^0.12.7",
    "prom-client": "~14.1.0",
    "prometheus-api-metrics": "^3.2.2",
    "uuid": "^8.3.2",
    "uuid-apikey": "^1.5.3",
    "winston": "^3.7.2",
    "winston-aws-cloudwatch": "^3.0.0"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "eslint": "^8.22.0",
    "jest": "^29.6.2",
    "jest-html-reporters": "^3.1.4",
    "jest-junit": "^16.0.0",
    "jest-summary-reporter": "^0.0.2",
    "migrate": "^1.8.0",
    "prettier": "^2.7.1",
    "sonarqube-scanner": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
